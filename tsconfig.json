{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "constant.js",
    "app/home.jsx",
    "components/ui/search-input.jsx",
    "config.js",
    "hooks/use-fetch-data.js",
    "app/index.jsx",
    "app/[id].jsx"
  ]
}
